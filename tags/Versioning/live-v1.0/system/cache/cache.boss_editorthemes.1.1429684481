		$('head').append('<link id="link_' + idSelect + '" rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=' + font + '">');
	}
	if(idSelect.val() != 'default'){
		$(class_name).css('font-family',font);
	}else{
		$(class_name).css('font-family','');
	}
}

